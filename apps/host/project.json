{
  "name": "host",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/host/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/host/browser",
        "index": "apps/host/src/index.html",
        "baseHref": "/",
        "main": "apps/host/src/main.ts",
        "polyfills": "apps/host/src/polyfills.ts",
        "tsConfig": "apps/host/tsconfig.app.json",
        "assets": ["apps/host/src/favicon.ico", "apps/host/src/assets"],
        "styles": ["apps/host/src/styles.scss"],
        "scripts": [],
        "webpackConfig": "apps/host/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/host/src/environments/environment.ts",
              "with": "apps/host/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "apps/host/webpack.config.prod.js"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/react:module-federation-ssr-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "browserTarget": "host:build:development",
        "serverTarget": "host:serve-server:development",
        "port": 4000,
        "browserTargetOptions": {
          "watch": true
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "browserTarget": "host:build:production",
          "serverTarget": "host:serve-server:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/host/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/host/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "serve-static": {
      "executor": "@nrwl/web:file-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "host:build",
        "port": 4000
      },
      "configurations": {
        "development": {
          "buildTarget": "host:build:development"
        },
        "production": {
          "buildTarget": "host:build:production"
        }
      }
    },
    "server": {
      "dependsOn": ["build"],
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "main": "apps/host/server.ts",
        "outputPath": "dist/apps/host/server",
        "tsConfig": "apps/host/tsconfig.server.json",
        "compiler": "babel",
        "externalDependencies": "all",
        "outputHashing": "none",
        "webpackConfig": "apps/host/webpack.server.config.js"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "sourceMap": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/host/src/environments/environment.ts",
              "with": "apps/host/src/environments/environment.prod.ts"
            }
          ],
          "sourceMap": false
        }
      }
    },
    "serve-browser": {
      "executor": "@nrwl/react:module-federation-dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "host:build",
        "hmr": true,
        "port": 4000
      },
      "configurations": {
        "development": {
          "buildTarget": "host:build:development"
        },
        "production": {
          "buildTarget": "host:build:production",
          "hmr": false
        }
      }
    },
    "serve-server": {
      "executor": "@nrwl/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "host:server:development",
        "buildTargetOptions": {
          "watch": true
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "buildTarget": "host:server:production"
        }
      }
    }
  },
  "tags": []
}
